{
    "name": "phpwintools/wmi-scripting",
    "type": "library",
    "description": "A PHP package for using WMI tools.",
    "keywords": [
        "windows",
        "management",
        "instrumentation",
        "Windows Management Instrumentation",
        "win",
        "win32",
        "wmi",
        "scripting",
        "com",
        "dotnet",
        "php"
    ],
    "license": "MIT",
    "version": "0.0.1-alpha",
    "authors": [
        {
            "name": "Joe Springe",
            "email": "jspringe@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "ext-com_dotnet": "*",
        "tightenco/collect": "^5.6",
        "illuminate/contracts": "^5.6"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.5"
    },
    "suggest": {
        "ext-com_dotnet": "Required for the library to work, but cannot be required due to TravisCI :("
    },
    "autoload": {
        "psr-4": {
            "PhpWinTools\\": "src/"
        },
        "files": [
            "src/WmiScripting/Support/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
